<catel:Window x:Class="DLR.WPF.Views.MainWindow"
              xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
              xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
              xmlns:catel="http://schemas.catelproject.com"
              xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
              ResizeMode="CanResize">
    <catel:Window.Resources>
    </catel:Window.Resources>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="50"/>
        </Grid.RowDefinitions>
        <ScrollViewer>
            <StackPanel x:Name="LayoutRoot">
                <StackPanel x:Name="AuthStack" Orientation="Horizontal">
                    <Button Content="Войти" Command="{Binding AuthenticateCommand}" Margin="0 0 10 0"/>
                    <TextBlock Text="{Binding UserRegion}"/>
                </StackPanel>
                <StackPanel Orientation="Horizontal" IsEnabled="{Binding Authenticated}">
                    <Button Content="Создать новое дело"/>
                    <Label Content="№ общего дела: "/>
                    <TextBlock Text="123456" VerticalAlignment="Center" Margin="0 0 10 0"/>
                    <Button Content="Загрузить сторое дело"/>
                </StackPanel>
                <StackPanel Orientation="Horizontal">
                    <Label Content="Тип акта: "/>
                    <ComboBox ItemsSource="{Binding ActTypes}" SelectedIndex="{Binding SelectedActIndex}"/>
                </StackPanel>
                <StackPanel IsEnabled="{Binding EditingEnabled}">
                    <StackPanel x:Name="Inspection" Visibility="Collapsed">
                        <TextBlock TextWrapping="Wrap" Text="Акт обследования:"/>
                        <Border x:Name="InspectionAct"  BorderThickness="1" BorderBrush="Black">
                        <StackPanel>
                            <TextBlock TextWrapping="Wrap" Text="Информация об акте проверки" HorizontalAlignment="Center" FontSize="15"/>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition />
                                    <RowDefinition />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="2*"/>
                                </Grid.ColumnDefinitions>
                                    <TextBlock TextWrapping="Wrap" Text="Дата составления акта" Grid.Row="0" Grid.Column="0"/>
                                <DatePicker Grid.Row="0" Grid.Column="1"/>

                                <TextBlock TextWrapping="Wrap" Text="Наименование органа муниципального земельного контроля" Grid.Row="1" Grid.Column="0"/>
                                <TextBox Text="" Grid.Row="1" Grid.Column="1" TextWrapping="Wrap"/>

                                <TextBlock TextWrapping="Wrap" Text="Номер акта" Grid.Row="2" Grid.Column="0"/>
                                <TextBox Text="" Grid.Row="2" Grid.Column="1" TextWrapping="Wrap"/>

                                <TextBlock TextWrapping="Wrap" Text="Место составления и время" Grid.Row="3" Grid.Column="0"/>
                                <TextBox Text="" Grid.Row="3" Grid.Column="1" TextWrapping="Wrap"/>
                                </Grid>
                        </StackPanel>
                        </Border>
                        <Border x:Name="InspectionEarthAreaObservation"  BorderThickness="1" BorderBrush="Black">
                            <StackPanel>
                                <TextBlock TextWrapping="Wrap" Text="Обследование земельного участка" HorizontalAlignment="Center" FontSize="15"/>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="2*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock TextWrapping="Wrap" Text="Основание проведения (указываются реквизиты основания для проведения обследования)" Grid.Row="0" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="0" Grid.Column="1" TextWrapping="Wrap"/>

                                    <TextBlock TextWrapping="Wrap" Text="Обследование земельного участка провели: (должность, фамилия, имя, отчество, должность должностного лица(должностных лиц), проводившего обследование)" Grid.Row="1" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="1" Grid.Column="1" TextWrapping="Wrap"/>

                                    <TextBlock TextWrapping="Wrap" Text="Лицо(а), участвовавшее(ие) в обследовании (фамилия, имя, отчество, должность участвующего(их) в обследовании земельного участка)" Grid.Row="2" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="2" Grid.Column="1" TextWrapping="Wrap"/>
                                </Grid>
                            </StackPanel>
                        </Border>
                        <Border x:Name="InspectionEarthArea"  BorderThickness="1" BorderBrush="Black">
                            <StackPanel>
                                <TextBlock TextWrapping="Wrap" Text="Земельный участок" HorizontalAlignment="Center" FontSize="15"/>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="*"/>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="2*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock TextWrapping="Wrap" Text="Кадастровый номер земельного участка" Grid.Row="0" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="0" Grid.Column="1" TextWrapping="Wrap"/>

                                    <TextBlock TextWrapping="Wrap" Text="Адрес обследуемой территории" Grid.Row="1" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="1" Grid.Column="1" TextWrapping="Wrap"/>

                                    <TextBlock TextWrapping="Wrap" Text="Площадь земельного участка" Grid.Row="2" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="2" Grid.Column="1" TextWrapping="Wrap"/>

                                    <TextBlock TextWrapping="Wrap" Text="Земельный участок предоставлен" Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Center"/>

                                    <TextBlock TextWrapping="Wrap" Grid.Row="4" Grid.Column="0" Text="(наименование юридического лица, Ф.И.О. индивидуального предпринимателя, Ф.И.О.физического лица, основание предоставления-указать название, дату и номер документа о предоставлении земельного участка)"/>
                                    <TextBox Text="" Grid.Row="4" Grid.Column="1" TextWrapping="Wrap"/>
                                </Grid>
                            </StackPanel>
                        </Border>
                        <Border x:Name="InspectionInstriments"  BorderThickness="1" BorderBrush="Black">
                            <StackPanel>
                                <TextBlock TextWrapping="Wrap" Text="Инструменты" HorizontalAlignment="Center" FontSize="15"/>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="2*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock TextWrapping="Wrap" Text="Марка фотоаппарата, ИД параметры инструмента" Grid.Row="0" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="0" Grid.Column="1" TextWrapping="Wrap"/>
                                </Grid>
                            </StackPanel>
                        </Border>
                        <Border x:Name="InspectionResearchResults"  BorderThickness="1" BorderBrush="Black">
                            <StackPanel>
                                <TextBlock TextWrapping="Wrap" Text="В ходе обследования установлено" HorizontalAlignment="Center" FontSize="15"/>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="2*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock TextWrapping="Wrap" Text="(описание территории, строений, сооружений, ограждения и т.д., наименование юридического лица, Ф.И.О.индивидуального предпринимателя, Ф.И.О.физического лица, использующего земельный участок, площадь земельного участка(фактическая, самовольно занятая), сведения о выявленных нарушениях требований законодательства и(или) требований установленных правовыми актами города Перми)" Grid.Row="0" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="0" Grid.Column="1" TextWrapping="Wrap"/>
                                </Grid>
                            </StackPanel>
                        </Border>
                        <Border x:Name="InspectionActAnnexes"  BorderThickness="1" BorderBrush="Black">
                            <StackPanel>
                                <TextBlock TextWrapping="Wrap" Text="Приложения к акту" HorizontalAlignment="Center" FontSize="15"/>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="2*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock TextWrapping="Wrap" Text="(фотоснимки, схема земельного участка, обмер земельного участка)" Grid.Row="0" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="0" Grid.Column="1" TextWrapping="Wrap"/>
                                </Grid>
                            </StackPanel>
                        </Border>
                    </StackPanel>
                    <StackPanel x:Name="AreaMeasurement" Visibility="Collapsed">
                        <TextBlock TextWrapping="Wrap" Text="Акт обмера площади земельного участка:"/>
                        <Border x:Name="AreaMeasurementAct"  BorderThickness="1" BorderBrush="Black">
                            <StackPanel>
                                <TextBlock TextWrapping="Wrap" Text="Информация об акте проверки" HorizontalAlignment="Center" FontSize="15"/>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="2*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock TextWrapping="Wrap" Text="Приложение к акту проверки от" Grid.Row="0" Grid.Column="0"/>
                                    <DatePicker Grid.Row="0" Grid.Column="1"/>

                                    <TextBlock TextWrapping="Wrap" Text="Наименование органа муниципального земельного контроля" Grid.Row="1" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="1" Grid.Column="1" TextWrapping="Wrap"/>

                                    <TextBlock TextWrapping="Wrap" Text="Номер акта проверки" Grid.Row="2" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="2" Grid.Column="1" TextWrapping="Wrap"/>
                                </Grid>
                            </StackPanel>
                        </Border>
                        <Border x:Name="AreaMeasurementPersons"  BorderThickness="1" BorderBrush="Black">
                            <StackPanel>
                                <TextBlock TextWrapping="Wrap" Text="Лица, учавствовавшие в обмере" HorizontalAlignment="Center" FontSize="15"/>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="2*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock TextWrapping="Wrap" Text="Обмер земельного участка произвели: (должность, Ф.И.О. должностного лица,производившего обмер земельного участка)" Grid.Row="0" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="0" Grid.Column="1" TextWrapping="Wrap"/>

                                    <TextBlock TextWrapping="Wrap" Text="В присутствии (должность, наименование юридического лица, Ф.И.О. законного представителя юридического лица, Ф.И.О. физического лица)" Grid.Row="1" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="1" Grid.Column="1" TextWrapping="Wrap"/>
                                </Grid>
                            </StackPanel>
                        </Border>
                        <Border x:Name="AreaMeasurementMeasurement"  BorderThickness="1" BorderBrush="Black">
                            <StackPanel>
                                <TextBlock TextWrapping="Wrap" Text="Обмер" HorizontalAlignment="Center" FontSize="15"/>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="2*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock TextWrapping="Wrap" Text="Адрес (кадастровый номер земельного участка, его площадь, вид разрешенного использования, местоположение земельного участка)" Grid.Row="0" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="0" Grid.Column="1" TextWrapping="Wrap"/>

                                    <TextBlock TextWrapping="Wrap" Text="Измерительный инструмент (указывается марка, идентификационные параметры измерительного инструмента)" Grid.Row="1" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="1" Grid.Column="1" TextWrapping="Wrap"/>

                                    <TextBlock TextWrapping="Wrap" Text="Площадь согласно обмеру (площадь земельного участка прописью БЕЗ кв.м)" Grid.Row="2" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="2" Grid.Column="1" TextWrapping="Wrap"/>

                                    <TextBlock TextWrapping="Wrap" Text="Расчет площади" Grid.Row="3" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="3" Grid.Column="1" TextWrapping="Wrap"/>

                                    <TextBlock TextWrapping="Wrap" Text="Особые отметки" Grid.Row="4" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="4" Grid.Column="1" TextWrapping="Wrap"/>
                                </Grid>
                            </StackPanel>
                        </Border>
                    </StackPanel>
                    <StackPanel x:Name="PhotoTable" Visibility="Collapsed">
                        <TextBlock TextWrapping="Wrap" Text="Фото таблица:"/>
                        <Border x:Name="PhotoTableAct"  BorderThickness="1" BorderBrush="Black">
                            <StackPanel>
                                <TextBlock TextWrapping="Wrap" Text="Информация об акте проверки" HorizontalAlignment="Center" FontSize="15"/>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="2*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock TextWrapping="Wrap" Text="Приложение к акту проверки от" Grid.Row="0" Grid.Column="0"/>
                                    <DatePicker Grid.Row="0" Grid.Column="1"/>

                                    <TextBlock TextWrapping="Wrap" Text="Наименование органа муниципального земельного контроля" Grid.Row="1" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="1" Grid.Column="1" TextWrapping="Wrap"/>

                                    <TextBlock TextWrapping="Wrap" Text="Номер акта проверки" Grid.Row="2" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="2" Grid.Column="1" TextWrapping="Wrap"/>
                                </Grid>
                            </StackPanel>
                        </Border>
                        <Border x:Name="PhotoTablePersons"  BorderThickness="1" BorderBrush="Black">
                            <StackPanel>
                                <TextBlock TextWrapping="Wrap" Text="Информация об участниках" HorizontalAlignment="Center" FontSize="15"/>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="2*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock TextWrapping="Wrap" Text="В отношении кого (наименование юридического лица, Ф.И.О. должностного лица, индивидуального предпринимателя, Ф.И.О.гражданина)" Grid.Row="0" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="0" Grid.Column="1" TextWrapping="Wrap"/>

                                    <TextBlock TextWrapping="Wrap" Text="Кто составил таблицу (Ф.И.О. должностного лица, составившего фототаблицу)" Grid.Row="1" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="1" Grid.Column="1" TextWrapping="Wrap"/>
                                </Grid>
                            </StackPanel>
                        </Border>
                        <Border x:Name="PhotoTablePhotos"  BorderThickness="1" BorderBrush="Black">
                            <StackPanel>
                                <TextBlock TextWrapping="Wrap" Text="Фотографирование" HorizontalAlignment="Center" FontSize="15"/>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="2*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock TextWrapping="Wrap" Text="Земельный участок (кадастровый номер земельного участка, его площадь, вид разрешенного использования, местоположение земельного участка)" Grid.Row="0" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="0" Grid.Column="1" TextWrapping="Wrap"/>

                                    <TextBlock TextWrapping="Wrap" Text="Фотографирование производилось (указывается марка, идентификационные параметры фотоаппарата)" Grid.Row="1" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="1" Grid.Column="1" TextWrapping="Wrap"/>
                                    
                                    <TextBlock TextWrapping="Wrap" Text="Снимки" Grid.Row="2" Grid.Column="0"/>
                                    <StackPanel Orientation="Vertical" Grid.Row="2" Grid.Column="1">
                                        <StackPanel Orientation="Horizontal">
                                            <Button Content="Добавить снимок"/>
                                            <Button Content="Удалить снимок"/>
                                        </StackPanel>
                                        <ListBox >
                                            <ListBox.ItemTemplate>
                                                <DataTemplate>
                                                    <StackPanel Orientation="Horizontal">
                                                        <TextBox Text="{Binding Id}"/>
                                                        <TextBox Text="{Binding Description}"/>
                                                    </StackPanel>
                                                </DataTemplate>
                                            </ListBox.ItemTemplate>
                                        </ListBox>
                                    </StackPanel>
                                </Grid>
                            </StackPanel>
                        </Border>
                    </StackPanel>
                    <StackPanel x:Name="Protocol" Visibility="Collapsed">
                        <TextBlock TextWrapping="Wrap" Text="Протокол:"/>
                        <Border x:Name="ProtocolDescription"  BorderThickness="1" BorderBrush="Black">
                            <StackPanel>
                                <TextBlock TextWrapping="Wrap" Text="Описание протокола" HorizontalAlignment="Center" FontSize="15"/>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="2*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock TextWrapping="Wrap" Text="Дата и время" Grid.Row="0" Grid.Column="0"/>
                                    <DatePicker Grid.Row="0" Grid.Column="1"/>

                                    <TextBlock TextWrapping="Wrap" Text="Номер протокола" Grid.Row="1" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="1" Grid.Column="1" TextWrapping="Wrap"/>

                                    <TextBlock TextWrapping="Wrap" Text="должность, Ф.И.О. должностного лица, составившего предписание" Grid.Row="2" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="2" Grid.Column="1" TextWrapping="Wrap"/>

                                    <TextBlock TextWrapping="Wrap" Text="Место составления" Grid.Row="2" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="2" Grid.Column="1" TextWrapping="Wrap"/>
                                </Grid>
                            </StackPanel>
                        </Border>
                        <Border x:Name="ProtocolPresense"  BorderThickness="1" BorderBrush="Black">
                            <StackPanel>
                                <TextBlock TextWrapping="Wrap" Text="В присутствии" HorizontalAlignment="Center" FontSize="15"/>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="2*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock TextWrapping="Wrap" Text="(должность, наименование юридического лица, должностногo лица, Ф.И.О.законного представителя юридического лица, N доверенности, Ф.И.О. физического лица)" Grid.Row="0" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="0" Grid.Column="1" TextWrapping="Wrap"/>
                                </Grid>
                            </StackPanel>
                        </Border>
                        <Border x:Name="ProtocolViolation"  BorderThickness="1" BorderBrush="Black">
                            <StackPanel>
                                <TextBlock TextWrapping="Wrap" Text="Нарушение" HorizontalAlignment="Center" FontSize="15"/>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="2*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock TextWrapping="Wrap" Text="Описание нарушения" Grid.Row="0" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="0" Grid.Column="1" TextWrapping="Wrap"/>

                                    <TextBlock TextWrapping="Wrap" Text="Нарушение допущено (наименование юридического лица, руководитель, ИНН, юридический адрес, банковские реквизиты, телефоны, Ф.И.О. должностного лица, индивидуального предпринимателя, физического лица, ИНН, паспортные данные, адрес регистрации, телефон)" Grid.Row="1" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="1" Grid.Column="1" TextWrapping="Wrap"/>

                                    <TextBlock TextWrapping="Wrap" Text="Данные действия подпадают под часть __ статьи __ Кодекса Российской Федерации об административных правонарушениях." Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Center"/>

                                    <TextBlock TextWrapping="Wrap" Text="Часть" Grid.Row="3" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="3" Grid.Column="1" TextWrapping="Wrap"/>

                                    <TextBlock TextWrapping="Wrap" Text="Статья" Grid.Row="4" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="4" Grid.Column="1" TextWrapping="Wrap"/>
                                </Grid>
                            </StackPanel>
                        </Border>
                        <Border x:Name="ProtocolOther"  BorderThickness="1" BorderBrush="Black">
                            <StackPanel>
                                <TextBlock TextWrapping="Wrap" Text="Другое" HorizontalAlignment="Center" FontSize="15"/>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="2*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock TextWrapping="Wrap" Text="Объяснения  лица  (физического,  должностного,  законного представителя юридического лица), в   отношении которого   составлен протокол  об административном правонарушении" Grid.Row="0" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="0" Grid.Column="1" TextWrapping="Wrap"/>

                                    <TextBlock TextWrapping="Wrap" Text="Особые отметки" Grid.Row="1" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="1" Grid.Column="1" TextWrapping="Wrap"/>

                                    <TextBlock TextWrapping="Wrap" Text="Лицо, составившее протокол (Ф.И.О., телефон)" Grid.Row="2" Grid.Column="0"/>
                                    <TextBox Text="" Grid.Row="2" Grid.Column="1" TextWrapping="Wrap"/>
                                </Grid>
                            </StackPanel>
                        </Border>
                    </StackPanel>
                    <StackPanel x:Name="Regulation" Visibility="Collapsed">
                        <TextBlock TextWrapping="Wrap" Text="Предписание:"/>
                        <Border x:Name="RegulationDescription"  BorderThickness="1" BorderBrush="Black">
                            <StackPanel>
                                <TextBlock TextWrapping="Wrap" Text="Описание предписания" HorizontalAlignment="Center" FontSize="15"/>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="2*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock TextWrapping="Wrap" Text="Дата от" Grid.Row="0" Grid.Column="0"/>
                                    <DatePicker Grid.Row="0" Grid.Column="1"/>

